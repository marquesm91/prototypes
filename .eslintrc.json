{
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
